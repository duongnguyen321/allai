{
  "version": 2,

  "builds": [
    {
      "src": "server.min.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "db.json"
      }
    },
    {
      "src": "/client/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.min.js"
    },
    {
      "src": "/get-prompt-result",
      "methods": ["POST"]
    },
    {
      "src": "/chat-history",
      "methods": ["GET"]
    }
  ]
}
